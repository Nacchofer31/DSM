-- @path UMLMM=/DSM-E2/Metamodels/dsmUML.ecore

module UMLRefinement;
create OUT : UMLMM refining IN : UMLMM;
rule Diagrama2Base {
	from Videoclub : UMLMM!Diagrama
	to 
	
	Ba : UMLMM!Base(
		clases <- Videoclub.clases
	)
	
}
rule getter_and_setter {
	from
	s: UMLMM!Atributo
	to
	t: UMLMM!Atributo (
		tipo<-s.tipo	
	)
}

